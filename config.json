{
  "version": "1.0",
  "organization_units": {
    "南京硕博睿达企业管理有限公司": {
      "template_path": "templates/农业银行模板.csv",
      "header_row": 0,
      "start_row": 1,
      "field_mappings": {
        "姓名": {
          "source_column": "姓名",
          "target_column": "A",
          "transform": "none",
          "required": true
        },
        "卡号": {
          "source_column": "卡号",
          "target_column": "B",
          "transform": "card_number",
          "required": true
        },
        "金额": {
          "source_column": "金额",
          "target_column": "C",
          "transform": "amount_decimal",
          "required": true
        },
        "日期": {
          "source_column": "日期",
          "target_column": "D",
          "transform": "date_format",
          "required": true
        }
      },
      "fixed_values": {
        "E": "浙江省",
        "F": "工资代发"
      },
      "auto_number": {
        "enabled": true,
        "column": "A",
        "start_from": 1
      },
      "bank_branch_mapping": {
        "enabled": true,
        "source_column": "开户银行",
        "target_column": "G"
      },
      "month_type_mapping": {
        "enabled": true,
        "target_column": "H",
        "month_format": "{month}月收入",
        "bonus_value": "年终奖",
        "compensation_value": "补偿金"
      },
      "transformations": {
        "date": {
          "output_format": "YYYY-MM-DD"
        },
        "amount": {
          "decimal_places": 2,
          "rounding": "round"
        },
        "card_number": {
          "remove_formatting": true,
          "luhn_validation": true
        }
      },
      "validation_rules": {
        "required_fields": ["姓名", "卡号", "金额", "日期"],
        "data_types": {
          "金额": "numeric",
          "日期": "date"
        }
      }
    },
    "工商银行": {
      "template_path": "templates/complex_template.xlsx",
      "header_row": 4,
      "start_row": 5,
      "field_mappings": {
        "客户姓名": {
          "source_column": "姓名",
          "target_column": "A",
          "transform": "none",
          "required": true
        },
        "银行卡号": {
          "source_column": "卡号",
          "target_column": "B",
          "transform": "card_number",
          "required": true
        },
        "工资金额": {
          "source_column": "金额",
          "target_column": "C",
          "transform": "amount_decimal",
          "required": true
        },
        "工资日期": {
          "source_column": "日期",
          "target_column": "D",
          "transform": "date_format",
          "required": true
        }
      },
      "fixed_values": {
        "E": "北京市",
        "F": "工资"
      },
      "transformations": {
        "date": {
          "output_format": "YYYY-MM-DD"
        },
        "amount": {
          "decimal_places": 2,
          "rounding": "round"
        },
        "card_number": {
          "remove_formatting": true,
          "luhn_validation": true
        }
      },
      "validation_rules": {
        "required_fields": ["姓名", "卡号", "金额", "日期"],
        "data_types": {
          "金额": "numeric",
          "日期": "date"
        }
      }
    }
  }
}
